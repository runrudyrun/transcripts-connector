# Google OAuth 2.0 Credentials
GOOGLE_CLIENT_ID="YOUR_CLIENT_ID_HERE"
GOOGLE_CLIENT_SECRET="YOUR_CLIENT_SECRET_HERE"
GOOGLE_PROJECT_ID="YOUR_PROJECT_ID_HERE"

# TLDV API Key
TLDV_API_KEY="YOUR_TLDV_API_KEY_HERE"

# The ID of the Google Shared Drive where documents will be created.
SHARED_DRIVE_ID=
# The ID of the specific folder within the Shared Drive to store the documents.
SHARED_DRIVE_FOLDER_ID=

# Comma-separated list of keywords to ignore meetings by their title (case-insensitive).
# Example: 1:1,catch-up,personal,private
IGNORE_KEYWORDS=1:1,1-1,one-on-one,catch-up,performance review
